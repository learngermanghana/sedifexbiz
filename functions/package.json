{
  "name": "sedifex-functions",
  "private": true,
  "version": "0.2.0",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "serve": "supabase functions serve --env-file ../.env.supabase --no-auto-port --debug",
    "deploy": "npm run build && supabase functions deploy --project-ref $SUPABASE_PROJECT_REF",
    "test": "npm run check"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.2"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
